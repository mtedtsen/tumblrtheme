    %ul#posts
      #content
        {block:Posts}
        {block:Text}
        #text.post
          #footer
            %ul.like_and_reblog_buttons
              %li.button {ReblogButton}
              %li.button {LikeButton}
            %br
          %li#textcontent.posttext
            {block:Title}
            %h3
              %a#texttitlelink{:href => "{Permalink}"} {Title}
            {/block:Title}{Body}
        {/block:Text}
        
        {block:Quote}
        #quote.post
          #footer
            %ul.like_and_reblog_buttons
              %li.button {ReblogButton}
              %li.button {LikeButton}
            %br
          %li.postquote
            "{Quote}"
            {block:Source}
            .source -{Source}
            {/block:Source}
        {/block:Quote}
        
        {block:Link}
        #link.post
          #footer
            %ul.like_and_reblog_buttons
              %li.button {ReblogButton}
              %li.button {LikeButton}
            %br
          %li.postlink
            .linktitle
              <a href="{URL}" class="link" {Target}>{Name}
              %br
                â‡
            {block:Description}
            .linkdescription {Description}
            {/block:Description}
        {/block:Link}
        
        {block:Photo}
        #photo.post
          #footer
            %ul.like_and_reblog_buttons
              %li.button {ReblogButton}
              %li.button {LikeButton}
            %br
          %li.postphoto
            %img#photoreal{:alt => "{PhotoAlt}", :src => "{PhotoURL-500}"}/
            {block:Caption}
            .caption {Caption}
            {/block:Caption}
        {/block:Photo}
        {/block:Posts}

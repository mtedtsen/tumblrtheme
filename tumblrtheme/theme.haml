          :css
      html{
        margin:0;
        padding:0;
        height:100%;
        width:100%;
      }
      body{
        background-color: #43454A;
        font-family: 'Open Sans', sans-serif;
        margin:0;
        padding:0;
        height:100%;
        width:100%;
      }
      /*HEADER
---------------------------------------------*/
      #header{
        overflow:hidden;
        font-family:'Norican', cursive, sans-serif;
        letter-spacing: 1px;
        font-weight:400;
        color:#3C3C3B;
        font-size:3em;
        background-color:white;
        text-align: center;
        margin-left:auto;
        margin-right:auto;
        width:5.8em;
      }
      /*SUMMARY
--------------------------------------------*/
     #name{
       font-family:'Norican', cursive, sans-serif;
       font-weight:400;
       font-size:2em;
     }
     #summary{
       overflow:hidden;
       color:white; 
       text-align:center;
       background: #525252;
       margin-bottom:0em;
       margin-left:auto;
       margin-right:auto;
       width:17.5em;
     }
     #profpic{
       height:7em;
       width:7em;
       border-radius:3.5em;
       -moz-border-radius:3.5em;
       -webkit-border-radius:3.5em; 
       border: 0.3em solid white;
     }
     /*POSTS
---------------------------------------------*/
     #content{
       width:17.5em;
       margin-left:auto;
       margin-right:auto;
     }
     .post{
       background: white;
       text-align:center;
       margin-top:1em;
       margin-bottom:em;
       margin-left:auto;
       margin-right:auto;
       min-height:22.5em;
       width: 17.5em;
       overflow:hidden;
       border-radius:0.2em;  
     }
     #posts{
       list-style-type:none;  
       list-style-type:inside;
       padding-left:0;
     }



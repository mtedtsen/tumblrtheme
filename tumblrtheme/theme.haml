          :css
            html{
             margin:0;
              padding:0;
             
              height:100%;
              width:100%;
              
             
             
            }
            body{
              background-color: #43454A;
              font-family: 'Open Sans', sans-serif;
            
              margin:0;
              padding:0;
             
              height:100%;
              width:100%;
            }
            
            /*HEADER
            ---------------------------------*/
            #header{
              overflow:hidden;
              font-family:'Norican', cursive, sans-serif;
              letter-spacing: 1px;
              font-weight:400;
              color:#3C3C3B;
              font-size:3em;
            
              background-color:white;
              text-align: center;
             
              margin-left:auto;
              margin-right:auto;
              width:5.8em;
            
            }
            
            /*SUMMARY
            --------------------------------*/
            #name{
              font-family:'Norican', cursive, sans-serif;
              font-weight:400;
              font-size:2em;
            
            }
            #summary{
              overflow:hidden;
              color:white; 
              text-align:center;
              background: #525252;
            
              margin-bottom:0em;
              margin-left:auto;
              margin-right:auto;
              width:17.5em;
             
            }
            #profpic{
              height:7em;
              width:7em;
              border-radius:3.5em;
              -moz-border-radius:3.5em;
              -webkit-border-radius:3.5em;
            
             
              -webkit-transform: rotate(90deg);
              -moz-transform: rotate(90deg);
              -ms-transform: rotate(90deg);
              -o-transform: rotate(90deg);
              transform: rotate(90deg);
            
              
              border: 0.3em solid white;
            }
            /*HEADER PIC
            ---------------------------------*/
            
            #headerpic{
              height:22.5em;  
              width:45.7em;
            
              background-image: url("http://static.tumblr.com/isnbmsj/XpBng8co3/fieldoyellow.jpg");
              background-repeat: no-repeat;
              background-position: center;
              background-size:100% 100%;
            
              margin-left:-45.7em;
              margin-top:-22.5em;
              margin-bottom:0em;
             
            }
            
            /*POSTS
            -----------------------------------------------------*/
            
            .post{
              background: white;
              text-align:center;
              
              margin-top:1em;
              margin-left:auto;
              margin-right:auto;
              min-height:22.5em;
              width: 17.5em;
            
              overflow:hidden;
              
              border-radius:0.2em;
              
             
            }
            
            
            #posts{
              list-style-type:none;  
              list-style-type:inside;
              padding-left:0;
              
               text-overflow:ellipsis;
            }
            .container{
              /*height:22.5em;*/
              width:17.5em;
              text-align:center;
              
              margin-left:auto;
              margin-right:auto;
             
            }
            .wrapper {
              /*min-height: 100%;
              height: auto !important;
              height: 19.5em;*/
              margin: 0 auto -3em;
              width:auto;
              padding-bottom:2.5em;
              
              text-overflow:ellipsis;
             
            }
            .footer, .push {
              height: 3em;
              border-radius: 0 0 0.2em 0.2em;
              background-color:#F8F8F8;
            }
            /*TEXT SPECIFIC
            ---------------------------------*/
            #text{
              text-align:left;
              
              text-overflow:ellipsis;
            }
            #textcontent{
              padding-left:1em;    
              padding-right:1em;
              
            }
            
            #texttitlelink,a:link {
                text-decoration: none;
                color: black;
                font-size:1.4em;
            }
            /*FOOTER
            -----------------------------------*/
             .like_and_reblog_buttons {
                            border: 1px solid #e8e8e8;
                            border-radius: 3px;
                            list-style: none;
                        }
            
                        .like_and_reblog_buttons li {
                            float: right;
                            margin: 0;
                            padding: 13px 7px 0px 7px;
                            height: 20px;
                        }
                        #dateinfooter{
                            float:left;
                            font-size:0.7em;
                            color: #B0B0B0 ;
                            display:inline-block;
                            
                            padding-left:1em;
                            padding-top:0.5em;
                        }
                 
            {customCSS}
